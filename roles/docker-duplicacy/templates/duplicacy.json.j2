{
    "storages": [
        {
            "name": "{{ directories.name }}",
            "url": "gcd://{{ duplicacy.remote_folder }}",
            "encrypted": false,
            "credentials": {
                "gcd_token": "{{ duplicacy.gcd_token }}"
            }
        }
    ],
    "computers": [
        {
            "name": "{{ containers.host.subdomain }}",
            "repositories": [
                {
                    "index": 0,
                    "id": "{{ duplicacy.id }}",
                    "path": "{{ directories.container_root }}",
                    "storage": "{{ directories.name }}",
                    "global_options": "{{ duplicacy.global_options }}",
                    "options": "{{ duplicacy.options }}",
                    "report_url_enabled": false,
                    "report_url": "",
                    "report_url_on_failure": false,
                    "filters": true,
                    "job_log": "",
                    "job_code": "",
                    "job_note": "",
                    "job_time": ""
                }
            ]
        }
    ],
    "schedules": [
        {
            "email_password": "{{ duplicacy.email_password }}",
            "name": "{{ duplicacy.backup_name }}",
            "start": "{{ duplicacy.start_time }}",
            "max": "{{ duplicacy.max_run_time }}",
            "frequency": {{ duplicacy.frequency }},
            "days": "{{ duplicacy.days }}",
            "jobs": [
                {
                    "type": "backup",
                    "computer": "{{ containers.host.subdomain }}",
                    "repository": 0,
                    "source": "{{ directories.container_root }}",
                    "storage": "{{ directories.name }}",
                    "options": "{{ duplicacy.options }}",
                    "parallel": false,
                    "global_options": "{{ duplicacy.global_options }}",
                    "status_code": "",
                    "status_note": "",
                    "status_log": "",
                    "status_time": ""
                },
                {
                    "type": "check",
                    "computer": "{{ containers.host.subdomain }}",
                    "repository": 0,
                    "source": "",
                    "storage": "{{ directories.name }}",
                    "options": "{{ duplicacy.options }}",
                    "parallel": false,
                    "global_options": "{{ duplicacy.global_options }}",
                    "status_code": "",
                    "status_note": "",
                    "status_log": "",
                    "status_time": ""
                },
                {
                    "type": "prune",
                    "computer": "{{ containers.host.subdomain }}",
                    "repository": 0,
                    "source": "",
                    "storage": "{{ directories.name }}",
                    "options": "{{ duplicacy.keep_options }} -id {{ duplicacy.id }} {{ duplicacy.options }}",
                    "parallel": false,
                    "global_options": "{{ duplicacy.global_options }}",
                    "status_code": "",
                    "status_note": "",
                    "status_log": "",
                    "status_time": ""
                }
            ],
            "email_enabled": {{ duplicacy.email_enabled }},
            "email_summary": {{ duplicacy.email_summary }},
            "email_server": "{{ mail.smtp_host }}:{{ mail.smtp_port }}",
            "email_username": "{{ mail.smtp_user }}",
            "email_from": "{{ mail.smtp_user }}",
            "email_to": "{{ mail.smtp_user }}",
            "email_subject": "{{ duplicacy.email_subject }}",
            "email_max_lines": 0,
            "email_excluded_logs": ""
        }
    ],
    "encryption_data": "{{ duplicacy.encryption_data }}",
    "cli_version": "",
    "administration_token": "",
    "administration_token_expiration": 86400,
    "license_id": "{{ duplicacy.license_id }}"
}
